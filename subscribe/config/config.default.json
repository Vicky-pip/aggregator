{
  // —— 多通道推送配置 ——  
  "push": {
    "localpush": {
      "engine": "local",
      // 写成 GitHub Raw 的前缀，指向你的仓库 main 分支
      "domain": "https://raw.githubusercontent.com/Vicky-pip/aggregator/refs/heads/main",
      "items": {
        "xxx-clash": {
          "folderid": "subscribe/out",
          "fileid": "test.yaml"
        },
        "23-singbox": {
          "fileid": "test.yaml"
        },
        "45-v2ray": {
          "fileid": "test.yaml"
        },
        "crawledsubs": {
          "folderid": "subscribe/out",
          "fileid": "test.yaml"
        },
        "crawledproxies": {
          "folderid": "subscribe/out",
          "fileid": "test.yaml"
        }
      }
    }
  },

  // —— 订阅源（Domains） ——  
  "domains": [
    {
      "name": "机场",
      "sub": [
        "https://raw.githubusercontent.com/xxf098/sub/main/clash.yaml",
        "https://raw.githubusercontent.com/mfuu/v2ray/master/clash.yaml",
        "https://raw.githubusercontent.com/ermaozi/get_subscribe/main/subscribe/clash.yaml"
      ],
      "domain": "github.com",
      "enable": true,
      "rename": "",
      "include": "",
      "exclude": "",
      // 将解析结果推送到 localpush 通道
      "push_to": ["localpush"],
      "ignorede": true,
      "liveness": true,
      "rate": 2.5,
      "count": 2,
      "coupon": "",
      "secure": false,
      "renew": {
        "account": [
          {
            "email": "",
            "passwd": "",
            "ticket": {
              "enable": true,
              "autoreset": false,
              "subject": "",
              "message": "",
              "level": 1
            }
          }
        ],
        "plan_id": 3,
        "package": "",
        "method": 1,
        "coupon_code": "",
        "chatgpt": {
          "enable": false,
          "regex": "",
          "operate": "IN"
        }
      }
    }
  ],

  // —— 爬虫模块（Crawl） ——  
  "crawl": {
    "enable": true,
    "exclude": "",
    "threshold": 5,
    "singlelink": true,
    // 持久化到 items 中的 crawledsubs / crawledproxies
    "persist": {
      "subs": "crawledsubs",
      "proxies": "crawledproxies"
    },

    "config": {
      "rename": "",
      "include": "",
      "exclude": "",
      "xxxxxxx": ""
    },

    "telegram": {
      "enable": true,
      "pages": 5,
      "exclude": "",
      "users": {
        "channel": {
          "include": "机场,clash,订阅,v2ray,vless",
          "exclude": "",
          "config": {
            "rename": "TG机场源",
            "xxxxxx": ""
          },
          "push_to": ["localpush"]
        }
      }
    },

    "google": {
      "enable": false,
      "exclude": "",
      "notinurl": [],
      "push_to": []
    },

    "github": {
      "enable": true,
      "pages": 2,
      "exclude": "",
      "spams": [],
      "push_to": ["localpush"]
    },

    "twitter": {
      "enable": true,
      "users": {
        "username": {
          "enable": true,
          "num": 30,
          "include": "",
          "exclude": "",
          "config": {
            "rename": "twitter节点",
            "xxxxxx": ""
          },
          "push_to": ["localpush"]
        }
      }
    },

    "repositories": [
      {
        "enable": false,
        "username": "",
        "repo_name": "",
        "commits": 3,
        "exclude": "",
        "push_to": []
      }
    ],

    "pages": [
      {
        "enable": true,
        "url": "",
        "include": "",
        "exclude": "",
        "config": {
          "rename": ""
        },
        "push_to": ["localpush"]
      }
    ],

    "scripts": [
      {
        "enable": false,
        "script": "file#function",
        "params": {
          "persist": {
            "fileid": ""
          },
          "any": "xxx",
          "config": {
            "enable": true,
            "liveness": true,
            "exclude": "",
            "rename": "any节点",
            "push_to": ["localpush"]
          }
        }
      }
    ]
  },

  // —— 分组配置 ——  
  "groups": {
    "xxx": {
      "emoji": true,
      "list": true,
      "targets": {
        "clash": "xxx-clash",
        "singbox": "23-singbox",
        "v2ray": "45-v2ray"
      },
      "regularize": {
        "enable": false,
        "locate": true,
        "bits": 2
      }
    }
  }
}
